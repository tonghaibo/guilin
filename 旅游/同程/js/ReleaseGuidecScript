function tcGuidec_SendHeartCallBack$(o){if(o&&o.responseHeader&&o.results){if(o.responseHeader.MessageCount>0){for(var i in o.results.message){var m=o.results.message[i];if(m.MessageType==1){tcGuidecShowInvitedDialogue(m.ServiceName,m.Message);tcGuidec_AcceptedServiceId=m.ServiceId;window.focus();tcGuidec$("9fc9da9dd0a4aea9").focus();break;}}}}}
